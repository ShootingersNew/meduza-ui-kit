{
  "name": "meduza-pet-ui-kit",
  "private": false,
  "version": "0.3.5",
  "type": "module",
  "main": "./dist/meduza-pet-ui-kit.umd.js",
  "module": "./dist/meduza-pet-ui-kit.es.js",
  "files": [
    "dist",
    "dist/styles/variables.styl"
  ],
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/meduza-pet-ui-kit.es.js",
      "require": "./dist/meduza-pet-ui-kit.umd.js"
    },
    "./styles": "./dist/meduza-pet-ui-kit.css",
    "./variables": "./dist/styles/variables.styl",
    "./enums": "./dist/enums/index.ts"
  },
  "scripts": {
    "dev": "vite",
    "generate-stylus": "node scripts/generate-stylus.js",
    "copy-models": "node scripts/copy-enums.js",
    "build": "npm run generate-stylus && vue-tsc -b && vite build && npm run copy-models",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "vue": ">=3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.3",
    "@rollup/plugin-dynamic-import-vars": "^2.1.5",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@storybook/vue3": "^8.4.7",
    "@storybook/vue3-vite": "^8.4.7",
    "@types/node": "^22.10.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/eslint-plugin": "1.1.10",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.17.0",
    "eslint-plugin-storybook": "^0.11.1",
    "eslint-plugin-vue": "^9.32.0",
    "prettier": "^3.4.2",
    "storybook": "^8.4.7",
    "stylus": "^0.64.0",
    "stylus-loader": "^8.1.1",
    "typescript": "~5.6.2",
    "vite": "^6.0.3",
    "vite-plugin-dts": "^4.4.0",
    "vite-plugin-vue-devtools": "^7.6.5",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.10"
  },
  "peerDependencies": {
    "vue": ">=3"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
